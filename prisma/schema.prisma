generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id        String   @id
  title     String
  amount    Float
  category  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("transactions")
}